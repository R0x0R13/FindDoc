<AnchorPane prefHeight="680.0" prefWidth="424.0" style="-fx-background-color: #380000;" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.SignupClinic">
